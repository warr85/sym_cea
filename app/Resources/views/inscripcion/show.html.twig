{% extends 'base.html.twig' %}

{% block body %}
    <h1>Inscripcion</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <th>Uc</th>
                <th>Seccion</th>
            </tr>
            <tr>
                <td>{{ inscripcion.id }}</td>
                <td>{{ inscripcion.idOfertaAcademica.idMallaCurricularUc.idUnidadCurricularVolumen.idUnidadCurricular.nombre }}</td>
                <td>{{ inscripcion.idOfertaAcademica.idSeccion.nombre }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('inscripcion_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('ceapp_estudiante_inscripcion_edit', { 'id': inscripcion.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
